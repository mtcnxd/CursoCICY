.................... void TickInit(void) 
.................... { 
....................     setup_timer_0(RTCC_INTERNAL | RTCC_DIV_16); 
*
016C:  MOVLW  83
016E:  MOVWF  FD5
....................     set_timer0(TICK_COUNTER); 
0170:  MOVLW  6D
0172:  MOVWF  FD7
0174:  MOVLW  83
0176:  MOVWF  FD6
....................  
....................     enable_interrupts(INT_TIMER0); 
0178:  BSF    FF2.5
....................     enable_interrupts(GLOBAL); 
017A:  MOVLW  C0
017C:  IORWF  FF2,F
.................... } 
017E:  GOTO   08FE (RETURN)
